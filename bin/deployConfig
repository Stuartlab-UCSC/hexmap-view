#!/bin/bash
# Deploy the scripts and config, but not www.

$HEXMAP/bin/checkConfig.sh
if [ "$?" == 1 ]; then
    exit 1
fi
source $HEX_VIEWER_CONFIG

if [ "$BUNDLE" == true ]; then
    echo "sorry, cannot deploy from a bundled build"
    exit 1
fi

cd $HEXMAP

tar cf config.tar bin config
scp config.tar hexmap-new:dev
