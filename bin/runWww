#!/bin/bash
# Start the HTTP server for a hexmap installation

INSTALL=dev

source /cluster/home/swat/sec/runIncludes
export MONGO_URL=mongodb://localhost:$DB_PORT/$DB_NAME
touch $HEXMAP/www.log
mv $HEXMAP/www.log $HEXMAP/www.prev.log
cd $HEXMAP/www
$METEOR/meteor --port $PORT --settings $HEXMAP/settings.json &> $HEXMAP/www.log &
